#include <stdio.h>
int main()
{

	int n;
	int number,i;
	int count=0;
	printf("Enter a number to check its prime\n");
	scanf("%d",&number);

	
	
	for(i=1;i<=number;i++) //loop the divident 
	{
		if(number%i==0){
			count++;  
			
		}

 	} 
 	if(count==2){
 		printf("%d is a prime\n",number);
 	}else{
 		printf("%d is not a prime\n",number);
 	}

printf("\n");
return 0;
}

 